<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="IHMChannel.controllers.AddMemberPopUpController">
    <children>
        <VBox>
            <children>
                <HBox fx:id="hbox" alignment="CENTER_LEFT" style="-fx-background-color: #ffffff; -fx-border-width: 1px; -fx-border-color: #000000">
                    <children>
                        <TextField fx:id="searchBar" promptText="Rechercher..." style="-fx-background-color: transparent;" HBox.hgrow="ALWAYS" />
                        <Button fx:id="searchBtn" mnemonicParsing="false" style="-fx-background-color: transparent;" />
                    </children>
                </HBox>
                <ListView fx:id="usersListView" styleClass="list-cell" />
            </children>
            <stylesheets>
                <URL value="@../../IHMChannel/css/style.css" />
            </stylesheets>
        </VBox>
    </children>
</AnchorPane>
