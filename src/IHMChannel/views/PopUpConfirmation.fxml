<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>


<?import javafx.scene.layout.Pane?>
<VBox xmlns="http://javafx.com/javafx/" xmlns:fx="http://javafx.com/fxml/1" fx:controller="IHMChannel.controllers.PopUpConfirmationController">
    <children>
        <HBox alignment="BASELINE_CENTER">
            <children>
                <Text fx:id="textConfirm" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" />
            </children>
        </HBox>
        <Pane VBox.vgrow="ALWAYS"></Pane>
        <HBox>
            <children>
                <Pane HBox.hgrow="ALWAYS" />
                <Button fx:id="confirmerBtn" mnemonicParsing="false" text="Confirmer" />
                <Pane HBox.hgrow="ALWAYS" />
                <Button fx:id="annulerBtn" mnemonicParsing="false" text="Annuler" />
                <Pane HBox.hgrow="ALWAYS" />
            </children>
        </HBox>
    </children>
</VBox>
